<%= form_for @announcement do |f| %>
  <%= f.error_messages %>
  <%= f.hidden_field :chamber_id, :value => params[:chamber_id] %>
  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :content %><br />
    <%= f.text_area :content %>
  </p>
  <p><%= f.submit %></p>
<% end %>
