<%= form_for @proposal do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :content %><br />
    <%= f.text_area :content %>
  </p>
  <p>
    <%= f.hidden_field :creator_id, :value => current_user.id %>
	<%= f.hidden_field :chamber_id, :value => params[:chamber_id] %>
  </p>
  <p><%= f.submit %></p>
<% end %>
